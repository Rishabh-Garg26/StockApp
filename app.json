{
  "expo": {
    "name": "vmgfoods",
    "slug": "vmgfoods",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist": {
        "NSFaceIDUsageDescription": "This app uses the FaceId to login to your account.",
        "NSPhotoLibraryUsageDescription": "This app access your photos to assign selected photo to the receipt",
        "UIFileSharingEnabled": true
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.stock.vmgfoods",
      "buildNumber": "1.0.4"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fd71909e-0356-41df-9d9a-cabdec46fb5a"
      },
      "plugins": [
        [
          "expo-image-picker",
          {
            "photosPermission": "This app access your photos to assign selected photo to the receipt"
          }
        ]
      ]
    }
  }
}